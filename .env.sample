# Environment variables for crawl-vnexpress project

# Ollama embedding model host and model name
OLLAMA_HOST=http://localhost:11434
OLLAMA_EMBEDDING=nomic-embed-text
OLLAMA_CHAT_MODEL=gemma:2b

# Qdrant vector database settings
RANT_URL=http://localhost:6333
QDRANT_COLLECTION=<your-collection-name>
QDRANT_VECTOR_SIZE=768
QDRANT_DISTANCE=cosine

# Google Gemini API
GOOGLE_API_KEY=<your-api-key>
GEMINI_MODEL_NAME=gemini-2.0-flash

# LangSmith
LANGCHAIN_API_KEY=<your-api-key>
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=<your-project-name>  # You can name it whatever you want
